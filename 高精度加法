#include<bits/stdc++.h>
#include<iostream>
#include<cstdio>
#include<cstring>
using namespace std;
int a[600],b[600],c[600];
char a1[600],b1[600];
int main() {
	cin>>a1;
	cin>>b1;    //一般不会用gets读入，据说gets的源代码有bug
	int lena=strlen(a1),lenb=strlen(b1);
	for(int i=0;i<=lena-1;i++) a[lena-i]=a1[i]-48;  //倒序存储 
	for(int i=0;i<=lenb-1;i++) b[lenb-i]=b1[i]-48;
	int lenc=1;
	int x=0;
	while(lenc<=lena||lenc<=lenb) {
		c[lenc]=a[lenc]+b[lenc]+x;
		x=c[lenc]/10;
		c[lenc]%=10;
		lenc++;
	}
	c[lenc]=x;
	if(c[lenc]==0) lenc--;
	for(int i=lenc;i>=1;i--) {
		cout<<c[i];
	}
	cout<<endl;
	return 0;
}
